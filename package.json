    "bindings": "^1.5.0",
    "builtin-modules": "^3.3.0",
    "camelcase": "^8.0.0",
    "child_process": "^1.0.2",
    "commander": "^12.0.0",
    "decamelize": "^6.0.0",
    "eventemitter3": "^5.0.1",
    "file-uri-to-path": "^2.0.0",
    "js-tiktoken": "^1.0.11",
    "langchain": "^0.1.17",
    "langsmith": "^0.1.22",
    "openai": "^4.29.2",
    "os": "^0.1.2",
    "p-queue": "^8.0.1",
